{
    "manifest_version": 2,
    "name": "Blockly to SVG",
    "description": "Export any Blockly workspace to SVG",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "logo.png"
    },
    "permissions": [
        "tabs",
        "unlimitedStorage",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}